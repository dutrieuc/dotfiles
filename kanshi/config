profile docked {
    output eDP-1 enable position 1920,320 scale 1.25
    exec swaymsg output eDP-1 bg ~/Pictures/wallpapers/wallpapers-gen/cliff-right.png  fill
    output "Goldstar Company Ltd 27EA63 " position 0,0
    exec swaymsg "output 'Goldstar Company Ltd 27EA63 ' bg ~/Pictures/wallpapers/wallpapers-gen/cliff-center.png  fill"
    exec swaymsg workspace 2 output 'Goldstar Company Ltd 27EA63 '
